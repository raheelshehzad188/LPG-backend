# Database
DB_USER=root
DB_PASS=
DB_HOST=localhost
DB_NAME=lahore_property_db

# JWT (change in production)
SECRET_KEY=lpg_secret_key_change_in_production

# Gemini
GEMINI_API_KEY=your_key_here
GEMINI_MODEL_NAME=gemini-3-flash-preview
# Cache (system prompt) — gemini-1.5 required. Set to disable: ENABLE_CONTEXT_CACHE=false
GEMINI_CACHE_MODEL=gemini-1.5-flash-001
# Cache TTL 60 min (Gemini default), 55 min pe proactive re-create
GEMINI_CACHE_TTL_MINUTES=60
GEMINI_CACHE_REFRESH_MINUTES=55
# Min 32k tokens — kam ho to content pad ho jata hai
GEMINI_MIN_CACHE_TOKENS=32768
